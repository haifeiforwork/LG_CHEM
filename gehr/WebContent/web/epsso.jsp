<%@ page contentType="text/html; charset=utf-8" %><%@ page import="com.sns.jdf.util.WebUtil" %><%	/**	* UURL을 설정해준다. 	*/	String now_url = request.getRequestURL().toString();	String uurl = WebUtil.encode(now_url);	/**	* 사용자 정보	*/	//사용자 통합 아이디	String sso_id = (String)session.getAttribute("SSO_ID");	//사번	String system_id = (String)session.getAttribute("SYSTEM_ID");	/**	* SSO에서 전달받은 세션을 검사한다. 	* session이 존재하지 않는다면, 통합인증 페이지로 이동한다.	**/	if (system_id == null) {		response.sendRedirect("/initech/sso/login_exec.jsp?UURL="+uurl);		return;	}	// SSO에서 전달받은 아이디가 존재한다면 장식재 스펙인 시스템의 개별 인증 처리 코드 삽입	// 현재는 인증정보를 확인하기 위한 코드가 삽입됨.	else {		out.println(sso_id +"님(사번:"+system_id+"), eHR(97) 시스템에 입장하셨습니다.");	}%>